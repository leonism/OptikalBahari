# HTML: Cache for 1 hour at edge, require revalidation in browser
/*.html
  Cache-Control: public, max-age=0, s-maxage=3600, must-revalidate

# Assets: Cache immutable files for 1 year
/assets/*
  Cache-Control: public, max-age=31536000, immutable

# Feeds and Sitemaps: Cache for 1 hour
/*.xml
  Cache-Control: public, max-age=3600
